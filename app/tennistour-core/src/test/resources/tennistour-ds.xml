<datasources>
    <datasource jndi-name="java:/jboss/datasources/TennistourDS" pool-name="TennistourDS" enabled="true" use-java-context="true">
        <connection-url>jdbc:mysql://db:3306/tennistour</connection-url>
        <driver>mysql-connector-java.jar_com.mysql.jdbc.Driver_5_1</driver>
        <security>
            <user-name>tennistour</user-name>
            <password>tennistour</password>
        </security>
        <statement>
            <prepared-statement-cache-size>100</prepared-statement-cache-size>
            <share-prepared-statements>true</share-prepared-statements>
        </statement>
    </datasource>
</datasources>
